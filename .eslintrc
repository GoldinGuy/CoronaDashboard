{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "plugin:prettier/recommended",
    // "airbnb",
    // "airbnb/hooks",
    "prettier/react"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"],
        "extensions": [".js", ".jsx", ".ts", ".tsx", ".mdx"]
      }
    }
  },
  "rules": {
    "linebreak-style": "off"
  }
}
